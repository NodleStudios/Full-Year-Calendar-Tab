{
  "manifest_version": 3,
  "name": "Calendar Tab",
  "description": "Shows a calendar view every time you open a new tab.",
  "version": "1.0",
  "author": "NodleStudios LLC",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}
